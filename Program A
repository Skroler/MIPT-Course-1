//pop: ÑƒÐ´Ð°Ð»Ð¸Ñ‚ÑŒ Ð¸Ð· ÑÑ‚ÐµÐºÐ° Ð¿Ð¾ÑÐ»ÐµÐ´Ð½Ð¸Ð¹ ÑÐ»ÐµÐ¼ÐµÐ½Ñ‚ Ð¸ Ð²Ñ‹Ð²ÐµÑÑ‚Ð¸ ÐµÐ³Ð¾ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ðµ, Ð»Ð¸Ð±Ð¾ Ð²Ñ‹Ð²ÐµÑÑ‚Ð¸ Â«errorÂ», ÐµÑÐ»Ð¸ ÑÑ‚ÐµÐº Ð±Ñ‹Ð» Ð¿ÑƒÑÑ‚;
//back: ÑÐ¾Ð¾Ð±Ñ‰Ð¸Ñ‚ÑŒ Ð·Ð½Ð°Ñ‡ÐµÐ½Ð¸Ðµ Ð¿Ð¾ÑÐ»ÐµÐ´Ð½ÐµÐ³Ð¾ ÑÐ»ÐµÐ¼ÐµÐ½Ñ‚Ð° ÑÑ‚ÐµÐºÐ°, Ð»Ð¸Ð±Ð¾ Ð²Ñ‹Ð²ÐµÑÑ‚Ð¸ Â«errorÂ», ÐµÑÐ»Ð¸ ÑÑ‚ÐµÐº Ð¿ÑƒÑÑ‚;
//getSize: Ð²Ñ‹Ð²ÐµÑÑ‚Ð¸ ÐºÐ¾Ð»Ð¸Ñ‡ÐµÑÑ‚Ð²Ð¾ ÑÐ»ÐµÐ¼ÐµÐ½Ñ‚Ð¾Ð² Ð² ÑÑ‚ÐµÐºÐµ;
//clear: Ð¾Ð¿ÑƒÑÑ‚Ð¾ÑˆÐ¸Ñ‚ÑŒ ÑÑ‚ÐµÐº Ð¸ Ð²Ñ‹Ð²ÐµÑÑ‚Ð¸ Â«okÂ»;
//exit: Ð²Ñ‹Ð²ÐµÑÑ‚Ð¸ Â«byeÂ» Ð¸ Ð·Ð°Ð²ÐµÑ€ÑˆÐ¸Ñ‚ÑŒ Ñ€Ð°Ð±Ð¾Ñ‚Ñƒ.

int main() {
    Stack st;
    std::string data;
    while (data != "exit") {
        std::cin >> data;
        if (data == "push") {
            int x;
            std::cin >> x;
            st.push(x);
            std::cout << "ok" << std::endl;
        }
        else if (data == "pop") {
            if (st.IsEmpty()) {
                std::cout << "error" << std::endl;
            }
            else {
                std::cout << st.pop() << std::endl;
            }
        }
        else if (data == "back") {
            if (st.IsEmpty()) {
                std::cout << "error" << std::endl;
            }
            else {
                std::cout << st.back() << std::endl;
            }
        }
        else if (data == "size") {
            std::cout << st.getSize() << std::endl;
        }
        else if (data == "clear") {
            st.clear();
            std::cout << "ok" << std::endl;
        }
        else {
            break;
        }
    }
    std::cout << "bye" << std::endl;
}
